[distutils]
index-servers =
    gitlab-central
    gitlab-project

[gitlab-central]
repository = $CI_API_V4_URL/projects/$RP_CENTRAL_REGISTRY_PROJECT_ID/packages/pypi
username = gitlab-ci-token

[gitlab-project]
repository = $CI_API_V4_URL/projects/$CI_PROJECT_ID/packages/pypi
username = gitlab-ci-token
